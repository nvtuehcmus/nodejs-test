{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": true,
        "no-tautology-expression": true,
        "no-unused-expression": false,
        "no-unused-variable": true,
        "no-var-requires":false,
        "no-console": false,
        "class-name": false
    },
    "rulesDirectory": []
}